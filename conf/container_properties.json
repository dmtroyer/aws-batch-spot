{
  "command" : ["/usr/bin/FAHClientlk", "--help"],
  "image"   : "foldingathome/fah-gpu:21.11.0",

  "resourceRequirements": [
    {
      "type"  : "VCPU",
      "value" : "16"
    },
    {
      "type"  : "MEMORY",
      "value" : "8192"
    }
  ],

  "volumes": [
    {
      "host" : {
        "sourcePath" : "/data"
      },
      "name" : "data"
    }
  ],

  "environment" : [
    {
      "name"  : "VARNAME",
      "value" : "VARVAL"
    }
  ],

  "mountPoints" : [
    {
      "sourceVolume"  : "data",
      "containerPath" : "/data",
      "readOnly"     : false
    }
  ],

  "ulimits" : [
    {
      "hardLimit" : 1024,
      "name"     : "nofile",
      "softLimit" : 1024
    }
  ]
}
